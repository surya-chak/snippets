xlim($1)
ylim($2)
clim(-1e-5,1e-5)
xlabel(L"$3",FontSize=21)
ylabel(L"$4",FontSize=21)
title(L"$0",FontSize=21)
ax.minorticks_on()
ax.tick_params(axis="both",which="major",length=8,width=3)
ax.tick_params(axis="both",which="minor",length=5,width=2)
