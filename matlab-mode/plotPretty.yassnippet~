set(gca,'XMinorTick','on')
% set(gca,'XTick',[0.5,1.5,2.5])
% set(gca,'XTickLabel',[{'0.5','1.5','2.5'}])

set(gca,'YMinorTick','on')
% set(gca,'YTick',[0.5,1.5,2.5])
% set(gca,'YTickLabel',[{'0.5','1.5','2.5'}])
set(gca,'TickDir','out')
